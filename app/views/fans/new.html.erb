<div class="container">

  <div class="form-card">

    <%= simple_form_for @fan do |f| %>
        <%= f.input :name %>
        <%= f.input :description %>
        <%= f.input :price %>
        <%= f.input :photo, as: :file %>
        <%= f.button :submit, class: 'btn-gradient' %>
    <% end %>

  </div>

</div>
